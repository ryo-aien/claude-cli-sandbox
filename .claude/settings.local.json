{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(tree:*)",
      "Bash(ls:*)",
      "Bash(pwd:*)",
      "Bash(echo:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(git status:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git branch:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(npm test:*)",
      "Bash(npm run:*)",
      "Bash(node:*)",
      "Bash(python:*)",
      "Bash(python3:*)",
      "Bash(npm install:*)",
      "Bash(npm uninstall:*)",
      "Bash(yarn install:*)",
      "Bash(pip install:*)"
    ],
    "deny": [
      "Bash(rm -rf /:*)",
      "Bash(rm -rf /*:*)",
      "Bash(chmod 777 /:*)",
      "Bash(chmod -R 777 /:*)",
      "Bash(git push --force:*)",
      "Bash(git push -f:*)",
      "Bash(git reset --hard HEAD~:*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(sudo rm:*)",
      "Bash(sudo chmod:*)",
      "Bash(sudo chown:*)",
      "Bash(mkfs:*)",
      "Bash(dd if=:*)",
      "Bash(fdisk:*)",
      "Bash(eval:*)",
      "Bash(systemctl:*)",
      "Bash(reboot:*)",
      "Bash(shutdown:*)",
      "Bash(init:*)",
      "Bash(git push:*)",
      "Bash(git pull:*)",
      "Bash(rm -rf:*)"
    ],
    "defaultMode": "default"
  },
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": false,
    "allowUnsandboxedCommands": false,
    "network": {
      "allowLocalBinding": true,
      "allowAllUnixSockets": false
    }
  },
  "includeCoAuthoredBy": true,
  "spinnerTipsEnabled": true,
  "alwaysThinkingEnabled": false
}
